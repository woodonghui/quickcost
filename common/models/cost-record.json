{
    "name": "CostRecord",
    "base": "PersistedModel",
    "idInjection": true,
    "options": {
        "validateUpsert": true
    },
    "properties": {
        "date": {
            "type": "date",
            "required": true
        },
        "productid": {
            "type": "number",
            "required": true
        },
        "quantity": {
            "type": "number",
            "required": true,
            "mysql": {
                "columnName": "quantity",
                "dataType": "float"
            }
        },
        "salerecordid": {
            "type": "number",
            "required": true
        },
        "paid": {
            "type": "boolean",
            "required": true
        }
    },
    "validations": [],
    "relations": {
        "saleRecord": {
            "type": "belongsTo",
            "model": "SaleRecord",
            "foreignKey": "salerecordid"
        },
        "product": {
            "type": "belongsTo",
            "model": "Product",
            "foreignKey": "productid"
        }
    },
    "acls": [],
    "methods": {}
}
